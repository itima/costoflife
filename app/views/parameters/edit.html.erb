<h1>Editing parameter</h1>

<% form_for(@parameter) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :parameterid %><br />
    <%= f.text_field :parameterid %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @parameter %> |
<%= link_to 'Back', parameters_path %>