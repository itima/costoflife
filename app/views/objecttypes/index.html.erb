<h1>Listing objecttypes</h1>

<table>
  <tr>
    <th>Objecttypeid</th>
    <th>Title</th>
  </tr>

<% @objecttypes.each do |objecttype| %>
  <tr>
    <td><%=h objecttype.objecttypeid %></td>
    <td><%=h objecttype.title %></td>
    <td><%= link_to 'Show', objecttype %></td>
    <td><%= link_to 'Edit', edit_objecttype_path(objecttype) %></td>
    <td><%= link_to 'Destroy', objecttype, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New objecttype', new_objecttype_path %>