<h1>New part</h1>

<% form_for(@part) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :partid %><br />
    <%= f.text_field :partid %>
  </p>
  <p>
    <%= f.label :parttypeid %><br />
    <%= f.text_field :parttypeid %>
  </p>
  <p>
    <%= f.label :parttype %><br />
    <%= f.text_field :parttype %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', parts_path %>