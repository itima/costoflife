<h1>Listing routines</h1>

<table>
  <tr>
    <th>Routineid</th>
    <th>Title</th>
  </tr>

<% @routines.each do |routine| %>
  <tr>
    <td><%=h routine.routineid %></td>
    <td><%=h routine.title %></td>
    <td><%= link_to 'Show', routine %></td>
    <td><%= link_to 'Edit', edit_routine_path(routine) %></td>
    <td><%= link_to 'Destroy', routine, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New routine', new_routine_path %>